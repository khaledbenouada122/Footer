{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "storybook": "concurrently \"npm run watch:storybook\" \"npm run watch:build-tailwind-storybook\"",
    "watch:storybook": "storybook dev -p 6006",
    "watch:build-tailwind-storybook": "npx tailwindcss -c ./tailwind-storybook.config.js -i ./app/globals.css -o ./.storybook/tailwind.css --watch",
    "build-tailwind-storybook": "npx tailwindcss -c ./tailwind-storybook.config.js -i ./app/globals.css -o ./.storybook/tailwind.css",
    "build-storybook": "npm run build-tailwind-storybook && storybook build",
    "chromatic": "npx chromatic --exit-zero-on-changes"
  },
  "dependencies": {
    "dayjs": "^1.11.9",
    "next": "13.4.12",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^7.1.1",
    "@storybook/addon-actions": "^7.1.1",
    "@storybook/addon-controls": "^7.1.1",
    "@storybook/addon-designs": "^7.0.1",
    "@storybook/addon-essentials": "^7.1.1",
    "@storybook/addon-interactions": "^7.1.1",
    "@storybook/addon-links": "^7.1.1",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/addon-storysource": "^7.1.1",
    "@storybook/addon-viewport": "^7.1.1",
    "@storybook/blocks": "^7.1.1",
    "@storybook/nextjs": "^7.1.1",
    "@storybook/react": "^7.1.1",
    "@storybook/testing-library": "^0.2.0",
    "@tailwindcss/typography": "^0.5.9",
    "@types/node": "20.4.4",
    "@types/react": "18.2.16",
    "@types/react-dom": "18.2.7",
    "autoprefixer": "10.4.14",
    "chromatic": "^6.20.0",
    "concurrently": "^8.2.0",
    "eslint": "8.45.0",
    "eslint-config-next": "13.4.12",
    "eslint-plugin-storybook": "^0.6.13",
    "postcss": "8.4.27",
    "storybook": "^7.1.1",
    "storybook-addon-pseudo-states": "^2.1.0",
    "tailwindcss": "3.3.3",
    "typescript": "5.1.6"
  }
}
